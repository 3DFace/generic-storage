{
	"name": "dface/generic-storage",
	"description": "Generic storage structures (key-value, set, association) with mysql- and memory-persistence",
	"type": "library",
	"license": "MIT",
	"authors": [{
		"name": "Denis Ponomarev",
		"email": "ponomarev@gmail.com"
	}],
	"minimum-stability": "dev",
	"require": {
		"php": ">=7.1",
		"dface/criteria": "dev-master",
		"dface/mysql": "dev-master",
		"ext-mysqli": "*",
		"ext-json": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.4"
	},
	"autoload": {
		"psr-0": {
			"dface\\GenericStorage\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-0": {
			"dface\\GenericStorage\\": "tests/"
		}
	}
}
